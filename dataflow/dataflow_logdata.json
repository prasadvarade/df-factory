{
	"name": "dataflow_logdata",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "loagdata_Parquet",
						"type": "DatasetReference"
					},
					"name": "logdataparquet"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "loagdata_Parquet",
						"type": "DatasetReference"
					},
					"name": "logdataparquet2"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Id as string,",
				"          Correlationid as string,",
				"          Operationname as string,",
				"          Status as string,",
				"          Eventcategory as string,",
				"          Level as string,",
				"          Time as timestamp,",
				"          Subscription as string,",
				"          Eventinitiatedby as string,",
				"          Resourcetype as string,",
				"          Resourcegroup as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> logdataparquet",
				"logdataparquet filter(!(isNull(Resourcegroup))) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Id as string,",
				"          Correlationid as string,",
				"          Operationname as string,",
				"          Status as string,",
				"          Eventcategory as string,",
				"          Level as string,",
				"          Time as timestamp,",
				"          Subscription as string,",
				"          Eventinitiatedby as string,",
				"          Resourcetype as string,",
				"          Resourcegroup as string",
				"     ),",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          Id,",
				"          Correlationid,",
				"          Operationname,",
				"          Status,",
				"          Eventcategory,",
				"          Level,",
				"          Time,",
				"          Subscription,",
				"          Eventinitiatedby,",
				"          Resourcetype,",
				"          Resourcegroup",
				"     )) ~> logdataparquet2"
			]
		}
	}
}